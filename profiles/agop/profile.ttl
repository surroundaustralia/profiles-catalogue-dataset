<https://linked.data.gov.au/def/agop>
    a prof:Profile ;
    dcterms:contributor <https://orcid.org/0000-0002-8742-7730> ;
    dcterms:created ""^^xsd:date ;
    dcterms:creator <https://linked.data.gov.au/org/agldwg> ;
    dcterms:modified ""^^xsd:date ;
    dcterms:publisher <https://linked.data.gov.au/org/agldwg> ;
    dcterms:description """This is a profile of the Web Ontology Language (OWL) and the Data Catalogue Vocabulary (DCAT) that is required to be conformed to by ontologies to be published via the Australian Govenrment Linked Data Working Group systems.

This profile requires certain metadata be present for each ontology to allow for good cataloguing of ontologies. It doesn't place any restrictions on the content of the ontology."""@en ;
    dcterms:title "Australian Government Ontologies Profile" ;
    prof:hasResource [
        dcterms:title "Specification Document"@en ;
        dcterms:conformsTo <https://www.w3.org/TR/html52/> ;
        dcterms:format "text/html" ;
        prof:hasArtifact <file://specification.html> ;
        prof:hasRole role:specification
    ] ,
    [
        dcterms:title "Validator in SHACL"@en ;
        dcterms:conformsTo <https://www.w3.org/TR/shacl/> ;
        dcterms:format "text/turtle" ;
        prof:hasArtifact <file://validation.ttl> ;
        prof:hasRole role:validation
    ] ,
    [
        dcterms:title "Example of a valid ontology"@en ;
        dcterms:format "text/turtle" ;
        prof:hasArtifact <file://valid.ttl> ;
        prof:hasRole role:example
    ] ,
    [
        dcterms:title "Example of an invalid ontology"@en ;
        dcterms:description "This example ontology does not satisfy..."@en ;
        dcterms:format "text/turtle" ;
        prof:hasArtifact <file://invalid.ttl> ;
        prof:hasRole role:example
    ] ,
    [
        dcterms:title "Example of a second invalid ontology"@en ;
        dcterms:description "This example ontology does not satisfy..."@en ;
        dcterms:format "text/turtle" ;
        prof:hasArtifact <file://valid.2.ttl> ;
        prof:hasRole role:example
    ] ,
    [
        dcterms:title "Profile code repository"@en ;
        dcterms:description "Code repository storing all Profile artifacts"@en ;
        prof:hasArtifact <https://github.com/AGLDWG/agop/> ;
        prof:hasRole role:repository
    ] ;
    prof:isProfileOf <https://www.w3.org/TR/owl2-rdf-based-semantics/> , <https://www.w3.org/TR/vocab-dcat/> ;
.

